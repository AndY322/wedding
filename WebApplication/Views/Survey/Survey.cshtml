<script>
$(document).ready(function (){
    $("#location-btn").click(function (){
        $('html, body').animate({
            scrollTop: $("#location-section").offset().top
        }, 2000);
    });
    $("#survey-btn").click(function (){
        $('html, body').animate({
            scrollTop: $("#survey-section").offset().top
        }, 2000);
    });
});

$("#survey").submit(function(e) {
    $("#thank-you").removeClass("hidden");
    e.preventDefault();
    var formData = $(this).serialize();
    $.post("/survey/save", formData);
});

$("#will-participate").change(function () {
    $("#secondary-questions :input").each(function (index, element) {
        element.removeAttribute("disabled");
    });
});
$("#wont-participate").change(function () {
    disableSecondaryControls();
    $("#secondary-questions :input[type!=radio]").val("");
    $("#secondary-questions :input[type=radio]").prop("checked", false);
});
$( document ).ready(function() {
    if (!$("#will-participate").prop("checked")) {
        disableSecondaryControls();
    }
});
function disableSecondaryControls() {
    $("#secondary-questions :input").each(function (index, element) {
        element.setAttribute("disabled", true);
    });
}
</script>